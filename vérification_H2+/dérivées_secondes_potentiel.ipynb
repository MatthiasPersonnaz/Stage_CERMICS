   "source": [
    "D, V0, σ, R, r, β, η = var('D, V0, σ, R, r, β, η')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "D |--> D^3*V0*e^(-1/2*D^2/σ^2)/σ^6 - 3*D*V0*e^(-1/2*D^2/σ^2)/σ^4"
      ]
     },
     "execution_count": 16,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "V_noy_el(D) = V0*D/σ^2*exp(-D^2/2/σ^2)\n",
    "diff(V_noy_el, D, 2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "R |--> 3*R^2*β/(R^2 + η^2)^(5/2) - β/(R^2 + η^2)^(3/2)"
      ]
     },
     "execution_count": 18,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "V_nucl_nucl(R) = β/sqrt(η^2+R^2)\n",
    "diff(V_nucl_nucl, R, 2)"
   ]
  },
